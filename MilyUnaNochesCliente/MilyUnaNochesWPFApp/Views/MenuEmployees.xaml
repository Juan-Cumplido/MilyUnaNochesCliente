<Page x:Class="MilyUnaNochesWPFApp.Views.MenuEmployees"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MilyUnaNochesWPFApp.Views"
      mc:Ignorable="d" 
       d:DesignHeight="720" d:DesignWidth="1280">

    <DockPanel>
        <Grid>
            
            <Border>
                <Border.Background>
                    <ImageBrush ImageSource="/Images/Background/EmployeesBackground.png" Stretch="UniformToFill" />
                </Border.Background>
            </Border>
            <Image Name="img_goBack" Source="/Images/Icons/goBack.png" MouseDown="Image_MouseDownGoBack" 
                   Cursor="Hand" Margin="45,34,1164,609" RenderTransformOrigin="0.5,0.506"/>

            <Label x:Name="lbl_RegisterClien" Content="Registrar Empleado" FontFamily="Bodoni MT Condensed"
           Foreground="#ae7d5c"  FontSize="30"   MouseDown="Lbl_RegisterEmploye_Click"
           HorizontalAlignment="Left" Margin="721,233,0,0" 
            Cursor="Hand" 
           VerticalAlignment="Top" Height="44" Width="191"/>

            <Label x:Name="lbl_consultClient" Content="Consultar Empleados" FontFamily="Bodoni MT Condensed"
           Foreground="#ae7d5c"  FontSize="30" MouseDown="Lbl_ConsultEmploye_Click"
           HorizontalAlignment="Left" Margin="376,233,0,0" 
            Cursor="Hand" 
           VerticalAlignment="Top" Height="44" Width="186"/>


            <Frame x:Name="Frame" NavigationUIVisibility="Hidden"
         Margin="0,282,0,0"/>


        </Grid>
    </DockPanel>
</Page>
